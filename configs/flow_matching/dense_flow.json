{
    "models": {
        "denoiser": {
            "name": "DenseFlowModel",
            "args": {
                "model_channels": 1024,
                "cond_channels": 1024,
                "num_blocks": 24,
                "num_heads": 16,
                "mlp_ratio": 4,
                "patch_size": 8,
                "pe_mode": "ape",
                "use_fp16": true,
                "use_checkpoint": true,
                "share_mod": false,
                "qk_rms_norm": true,
                "qk_rms_norm_cross": true
            }
        }
    },
    "dataset": {
        "name": "ImageConditionedBlockCoords",
        "args": {
            "max_block_num": 110000,
            "min_aesthetic_score": 0.0
        }
    },
    "trainer": {
        "name": "ImageConditionedFlowMatchingCFGTrainer",
        "args": {
            "max_steps": 100000,
            "batch_size_per_gpu": 1,
            "batch_split": 1,
            "t_schedule": {
                "name": "logitNormal",
                "args": {
                    "mean": -0.8,
                    "std": 0.8
                }
            },
            "cond_noise_std": 0.01,
            "optimizer": {
                "name": "AdamW",
                "args": {
                    "lr": 0.0001,
                    "weight_decay": 0
                }
            },
             "lr_scheduler": {
                "name": "WarmupCosineLRScheduler",
                "args": {
                    "warmup_steps": 5000,
                    "total_steps": 500000,
                    "min_lr_ratio": 0.01
                }
            },
            "ema_rate": [0.999],
            "grad_clip": {
                "name": "AdaptiveGradClipper",
                "args": {
                    "max_norm": 1.0,
                    "clip_percentile": 95
                }
            },
            "i_log": 100,
            "num_samples": 16,
            "i_sample": 10,
            "i_save": 10000,
            "image_cond_model": "dinov2_vitl14_reg"
        }
    }
}
